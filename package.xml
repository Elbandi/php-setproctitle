<?xml version="1.0" encoding="UTF-8"?>
<package version="1.0">
  <name>setproctitle</name>
  <channel>pecl.php.net</channel>
  <summary>Set proc title</summary>
  <description>This extension provides setproctitle function for setting the invoking process's title.</description>
  <maintainer>
   <name>Andras Elso</name>
   <user>elso.andras</user>
   <email>elso.andras@gmail.com</email>
   <role>lead</role>
  </maintainer>
 <dependencies>
  <required>
   <php>
    <min>5.3.2</min>
   </php>
   <pearinstaller>
    <min>1.4.0</min>
   </pearinstaller>
  </required>
 </dependencies>
<release>
    <version>1.0</version>
    <date>2011-12-09</date>
    <license>PHP License</license>
    <state>stable</state>
    <notes>Initial release.</notes>
    <configureoptions>
      <configureoption name="with-setproctitle" default="autodetect" prompt="libsetproctitle installation directory?"/>
    </configureoptions>
  <filelist>
    <dir name="/">
      <file name="php_title.h" role="src" />
      <file name="title.c" role="src" />
      <file name="config.m4" role="src" />
    </dir>
  </filelist>
</release>
</package>
